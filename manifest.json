{
  "manifest_version": 3,
  "name": "JetMark",
  "version": "1.0.3",
  "description": "Acceso ultra r√°pido a tus marcadores con el teclado mediante comandos.",
  "permissions": [
    "bookmarks",
    "tabs"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Abrir JetMark",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Alt+B",
        "mac": "Command+Shift+K"
      }
    }
  }
}