
{
  "manifest_version": 3,
  "name": "JetMark",
  "version": "1.0.5",
  "description": "Lightning-fast command palette to search and launch browser bookmarks instantly via keyboard shortcuts.",
  "permissions": [
    "bookmarks"
  ],
  "action": {
    "default_popup": "index.html",
    "default_title": "Abrir JetMark"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Alt+Shift+J",
        "mac": "Command+Shift+J"
      },
      "description": "Abrir el buscador de JetMark"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
